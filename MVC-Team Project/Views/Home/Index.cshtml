                                <div class="flip-card-front">
                                    <img src="@i.ProfilePicture" alt="Doctor" class="doc-img" />
                                    <h5 class="doctor-name">Dr. @i.FullName</h5>
                                    <p class="specialty">Specialist</p>
                                </div>

                                <!-- Back -->
                                <div class="flip-card-back">
                                    <p><strong>Name:</strong> @i.FullName</p>
                                    <p><strong>Address:</strong> @i.ClinicAddress</p>
                                    <p><strong>License:</strong> @i.LicenseNumber</p>
                                    @* <a class="view-btn" asp-controller="Doctors" asp-action="Details" asp-route-Id="@i.Id">View Profile</a> *@
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>



    </div>
</section>


