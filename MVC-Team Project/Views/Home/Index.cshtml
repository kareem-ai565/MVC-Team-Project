<!-- Slides Section -->
<section class="slider-section" data-aos="zoom-in" id="#doctors">
    <div class="slider-container">
        <div class="slider-head">
            <p>MEET OUR EXPERIENCED TEAM</p>
            <h3>Our Dedicated Doctors Team</h3>
