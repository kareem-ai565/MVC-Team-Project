@using MVC_Team_Project.View_Models
@model List<RegisterDoctorViewModel>

@{
    ViewData["Title"] = "Home";
    Layout = "_Layout";
}
<style>


    .book-clean-section {
        padding: 80px 0;
        background: linear-gradient(to right, #033B48, #eef5f7);
        position: relative;
    }

    .content-box {
        max-width: 800px;
        margin: 0 auto;
    }

    .main-title {
        font-size: 2.8rem;
        font-weight: 800;
        color: #2f4f59;
    }

        .main-title span {
            color: #538493;
        }

    .description {
        font-size: 1.15rem;
        color: white;
        line-height: 1.8;
        max-width: 700px;
        margin: 0 auto;
    }

    .btn-book {
        background-color: #538493;
        color: white;
        padding: 14px 36px;
        font-size: 1.1rem;
        border-radius: 50px;
        text-decoration: none;
        font-weight: 600;
        transition: 0.3s ease;
        box-shadow: 0 8px 24px rgba(0, 0, 0, 0.1);
    }

        .btn-book:hover {
            background-color: #3d697a;
            transform: scale(1.05);
        }

    media (max-width: 768px) {
        .main-title

    {
        font-size: 2rem;
    }

    .btn-book {
        font-size: 1rem;
        padding: 12px 28px;
    }

    }






    .why-choose-us {
        background: linear-gradient(to bottom right, #f4f8fa, #ffffff);
    }

    .section-heading {
        font-size: 2.7rem;
        font-weight: 800;
        color: #033B48;
        position: relative;
        letter-spacing: 0.5px;
    }

        .section-heading::after {
            content: "";
            width: 80px;
            height: 4px;
            background-color: #538493;
            display: block;
            margin: 10px auto 0;
            border-radius: 4px;
        }

    .feature-card {
        background: #fff;
        border-radius: 16px;
        padding: 35px 25px;
        text-align: center;
        border: none;
        box-shadow: 0 10px 40px rgba(0, 0, 0, 0.08);
        transition: 0.3s ease-in-out;
    }

        .feature-card:hover {
            transform: translateY(-8px) scale(1.02);
            box-shadow: 0 20px 50px rgba(83, 132, 147, 0.2);
        }

    .icon-box {
        width: 70px;
        height: 70px;
        display: flex;
        align-items: center;
        justify-content: center;
        margin: 0 auto 20px;
        font-size: 30px;
        border-radius: 50%;
        color: white;
        box-shadow: 0 5px 15px rgba(0,0,0,0.1);
    }

    .bg1, .bg2, .bg3 {
        background-color: #033B48;
    }


    .feature-card h5 {
        font-weight: bold;
        font-size: 1.3rem;
        color: #2f4f59;
        margin-bottom: 10px;
    }

    .feature-card p {
        font-size: 0.95rem;
        color: #6c757d;
        line-height: 1.6;
    }

    media (max-width: 576px) {
        .section-heading

    {
        font-size: 2rem;
    }

    .feature-card {
        padding: 25px 18px;
    }

    }


    .doctors-heading {
        text-align: center;
        font-size: 2.5rem;
        font-weight: 800;
        background: linear-gradient(90deg, #033B48, #7aa9bc);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        margin-bottom: 40px;
        position: relative;
    }

        .doctors-heading::after {
            content: '';
            width: 80px;
            height: 4px;
            background-color: #033B48;
            display: block;
            margin: 10px auto 0;
            border-radius: 2px;
        }



    /* Swiper */
    .swiper {
        padding-bottom: 60px;
    }

    .swiper-slide {
        display: flex;
        justify-content: center;
    }
    .flip-card {
        background-color: transparent;
        width: 280px;
        height: 360px;
        perspective: 1000px;
        margin: auto;
    }

    .flip-card-inner {
        position: relative;
        width: 100%;
        height: 100%;
        text-align: center;
        transition: transform 0.8s;
        transform-style: preserve-3d;
    }

    .flip-card:hover .flip-card-inner {
        transform: rotateY(180deg);
    }

    .flip-card-front,
    .flip-card-back {
        position: absolute;
        width: 100%;
        height: 100%;
        border-radius: 15px;
        backface-visibility: hidden;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
        padding: 20px;
    }

    .flip-card-front {
        background: linear-gradient(to top, #e8f1f5, #ffffff);
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
    }

        .flip-card-front .doc-img {
            width: 100px;
            height: 100px;
            border-radius: 50%;
            object-fit: cover;
            border: 4px solid #538493;
            margin-bottom: 15px;
        }

    .doctor-name {
        font-size: 1.2rem;
        font-weight: bold;
        color: #033B48;
    }

    .specialty {
        color: #033B48;
        font-style: italic;
    }

    .flip-card-back {
        background-color: #033B48;
        color: white;
        transform: rotateY(180deg);
        border-radius: 15px;
        display: flex;
        flex-direction: column;
        justify-content: center;
        padding: 25px;
    }

        .flip-card-back h5 {
            margin-bottom: 10px;
            font-size: 18px;
        }

        .flip-card-back p {
            font-size: 14px;
            margin-bottom: 6px;
        }

    .view-btn {
        background-color: white;
        color: #538493;
        padding: 8px 16px;
        border-radius: 30px;
        text-decoration: none;
        font-weight: bold;
        margin-top: 10px;
        transition: 0.3s;
    }

        .view-btn:hover {
            background-color: #e0e0e0;
        }
</style>
